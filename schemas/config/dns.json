{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z_][a-zA-Z0-9_]*$": {
      "type": "object",
      "properties": {
        "Primary": {
          "type": "string",
          "format": "ipv4"
        },
        "Secondary": {
          "type": "string",
          "format": "ipv4"
        },
        "Primary6": {
          "type": "string",
          "format": "ipv6"
        },
        "Secondary6": {
          "type": "string",
          "format": "ipv6"
        }
      },
      "required": ["Primary", "Secondary", "Primary6", "Secondary6"],
      "additionalProperties": false
    }
  }
}
